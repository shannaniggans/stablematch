{
  "name": "FullStride",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
